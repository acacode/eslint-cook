{
  "name": "eslint-plugin-dynamic",
  "version": "1.0.17",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node src/generator.ts",
    "build": "rimraf dist && tsc",
    "prepare-package": "yarn build && yarn docs && yarn syncDeps",
    "docs": "ts-node docs/index.ts",
    "syncDeps": "ts-node syncDeps/index.ts",
    "prepublishOnly": "yarn prepare-package",
    "postinstall": "install-peerdeps eslint-plugin-dynamic --silent --only-peers"
  },
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "author": "acacode",
  "license": "ISC",
  "engines": {
    "node": ">=14"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn prepare-package && git add -A"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "dist"
  ],
  "peerEslintDependencies": {
    "prettier": "*",
    "eslint": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "peerDependencies": {},
  "dependencies": {
    "cosmiconfig": "^7.0.1",
    "install-peerdeps": "^3.0.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.6",
    "eta": "^1.12.3",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.6.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "bugs": {
    "url": "https://github.com/acacode/eslint-plugin-dynamic/issues"
  },
  "homepage": "https://github.com/acacode/eslint-plugin-dynamic",
  "repository": {
    "type": "git",
    "url": "git://github.com/acacode/eslint-plugin-dynamic"
  }
}
